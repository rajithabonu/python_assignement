def count_even_odd(arr):
   
    even_count = 0
    odd_count = 0
    for num in arr:
        if num % 2 == 0:
            even_count += 1
        else:
            odd_count += 1
    return even_count, odd_count
my_array = [1, 2, 3, 4, 5, 2, 6, 7, 8, 1, 9]
my_array2 = [2,4,6,8,10,12,14]

print("Even/Odd counts:", count_even_odd(my_array))
