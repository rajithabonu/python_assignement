class ChildClass(AbstractClass):  # Subclass of AbstractClass
    def abstract_method(self):  # Implementing abstract method
        print("Implementation of Abstract Method in Child Class")

    def call_abstract_method(self):
        self.abstract_method()  # Calling abstract method

# Main method
if __name__ == "__main__":
    obj = ChildClass()
    obj.call_abstract_method()  
